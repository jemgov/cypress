pipeline {
  agent any
  tools {
    nodejs "NodeJS 25"
  }
  stages {
    stage('Probar npm install') {
      steps {
        timeout(time: 5, unit: 'MINUTES') {
          dir('mi-proyecto-cypress') {
            powershell """
            Write-Host '=== Iniciando npm install ==='
            npm install
            Write-Host '=== npm install finalizado ==='
            """
          }
        }
      }
    }
  }
}
